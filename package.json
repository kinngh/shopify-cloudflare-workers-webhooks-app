{
  "name": "shopify-cloudflare-workers-webhooks-app",
  "version": "0.0.1",
  "description": "Boilerplate to offload your Shopify app's webhooks to Cloudflare Workers",
  "author": {
    "name": "Harshdeep Singh Hura",
    "url": "https://harshdeephura.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kinngh/shopify-cloudflare-workers-webhooks-app.git"
  },
  "scripts": {
    "dev": "wrangler dev",
    "publish": "wrangler deploy",
    "-----> Wrangler <-----": "",
    "wrangler": "wrangler",
    "w:login": "wrangler login",
    "w:init": "wrangler init",
    "-----> utils <-----": "",
    "pretty": "prettier --write ./",
    "ngrok": "ngrok start --all --config ./ngrok.yml",
    "update": "ncu -u",
    "-----> Shopify <-----": "",
    "g:install": "npm i -g @shopify/app@latest @shopify/cli@latest",
    "shopify": "shopify",
    "-----> Prisma <-----": "",
    "db:pull": "npx prisma db pull",
    "db:push": "npx prisma db push"
  },
  "devDependencies": {
    "npm-check-updates": "^16.14.15",
    "prettier": "^3.2.5",
    "prisma": "^5.9.1",
    "wrangler": "3.28.2"
  },
  "dependencies": {
    "@prisma/client": "5.9.1",
    "@shopify/shopify-api": "^9.2.0",
    "cryptr": "^6.3.0",
    "itty-router": "^4.0.27",
    "ngrok": "^5.0.0-beta.2"
  }
}
